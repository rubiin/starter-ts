{
  "name": "pkg-name",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/lib-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/lib-starter.git"
  },
  "bugs": "https://github.com/rubiin/lib-starter/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint '{src,test}/**/*.ts' --cache --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@antfu/ni": "^28.2.0",
    "@types/node": "^24.10.9",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "esno": "^4.8.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "dependencies": {}
}
