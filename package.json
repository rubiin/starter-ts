{
  "name": "pkg-name",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@10.17.0",
  "description": "",
  "author": "Rubin Bhandari <roobin.bhandari@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rubiin/lib-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rubiin/lib-starter.git"
  },
  "bugs": "https://github.com/rubiin/lib-starter/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint '{src,test}/**/*.ts' --cache --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.0",
    "@antfu/ni": "^0.23.0",
    "@types/node": "^22.0.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.39.0",
    "esno": "^0.17.0",
    "rimraf": "^5.0.0",
    "typescript": "^5.0.4",
    "unbuild": "^3.0.0",
    "vite": "^7.0.0",
    "vitest": "^3.0.0"
  }
}
